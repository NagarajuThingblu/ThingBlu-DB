<!-- Labeling Task Grid -->
<div class="ui-g" id="LABELING" *ngIf="SelectedTabName === 'LABELING'">
  <div class="ui-g-12">    
    <div class="ui-g-12 ui-g-nopad">
      <div class="ui-g-12 ui-md-12 ui-g-nopad">

          <!-- Available Tasks -->
          <app-section-header [headerLabel]="prodDashboardResource.lblAvailableTask"></app-section-header>
          <p-table #dtLabelingAvailableTask
            [value]="DashboardData.labeling?.availableTasks"
            styleClass="clsTableWithoutHeaderColorCss clsRemoveTableInnerBorders"
            [rowsPerPageOptions]="DashboardData.labeling?.availTasksPagination" 
            (onPage)="onAvailablePageChange($event)"
            [responsive]="true"
            rowHover="true"
            [paginator]="true" 
            [rows]="10"
            >
            <ng-template pTemplate="header">
              <tr>
                <th [pSortableColumn]="'StrainName'"> 
                  {{prodDashboardResource.lblStrain}}
                  <p-sortIcon [field]="'StrainName'"></p-sortIcon>
                </th>
                <th>{{prodDashboardResource.lblPkgSize}}</th>
                <th [pSortableColumn]="'OrderRefId'"> 
                  {{prodDashboardResource.lblOrder}}
                  <p-sortIcon [field]="'OrderRefId'"></p-sortIcon>
                </th>
                <th [pSortableColumn]="'TubeAvlQty'" style="white-space: nowrap;"> 
                  {{prodDashboardResource.lblReadyForLabeling}}
                  <p-sortIcon [field]="'TubeAvlQty'"></p-sortIcon>
                </th>
                <th class="clsRemoveFrmPrint clsAssignTaskColumn">&nbsp;</th>
              </tr>
              <tr class="clsRemoveFrmPrint">
                <th>
                  <input pInputText autocomplete="off" type="search" 
                    (input)="dtLabelingAvailableTask.filter($event.target.value, 'StrainName', 'contains')"
                    class="clsControl">
                </th>
                <th>
                    &nbsp;
                </th>
                <th>
                  <input pInputText autocomplete="off" type="search" 
                  (input)="dtLabelingAvailableTask.filter($event.target.value, 'OrderRefId', 'contains')"
                  class="clsControl">
                </th>
                <th>
                    &nbsp;
                </th>
                <th class="clsRemoveFrmPrint clsAssignTaskColumn">&nbsp;</th>
              </tr>
            </ng-template>
            <ng-template pTemplate="body" let-rowData let-i="rowIndex" >
              <tr>
                <!--Strain -->
                <td>
                  <span class="ui-column-title">{{prodDashboardResource.lblStrain}}</span>
                  {{rowData?.StrainName}}
                </td>
                <!--End Strain -->
                <!--Order Package Size -->
                <td>
                  <span class="ui-column-title">{{prodDashboardResource.lblPkgSize}}</span>
                  {{rowData?.PkgTypeName}}&nbsp;-&nbsp;{{rowData?.ItemQty}}&nbsp;x&nbsp;{{rowData?.UnitValue}} {{prodDashboardResource.unitGrams}}
                </td>
                <!--Order Package Size -->
                <!--OrderNo -->
                <td>
                    <span class="ui-column-title">{{prodDashboardResource.lblOrder}}</span>
                    {{rowData?.OrderRefId}}
                </td>
                <!--OrderNo -->
                <!--ReadyForLabeling -->
                <td>
                  <span class="ui-column-title">{{prodDashboardResource.lblReadyForLabeling}}</span>
                  {{rowData?.TubeAvlQty}}&nbsp;{{rowData?.PkgTypeName}}{{rowData?.TubeAvlQty > 1 && rowData?.PkgTypeName.toUpperCase() !== 'JOINTS' ? 's' : ''}}
                </td>
                <!--ReadyForLabeling -->
                <!--Claim Button -->
                <td class="clsRemoveFrmPrint clsAssignTaskColumn" >
                  <p-button type="Button" styleClass="ui-button-rounded"  
                    label="{{prodDashboardResource.btnClaim}}" (click)="onLabelingClaim(rowData)">
                  </p-button>
                </td>
                <!--Claim Button -->                
              </tr>
            </ng-template>
            <ng-template pTemplate="emptymessage">
              <tr>
                <td [attr.colspan]="5">
                  {{ globalResource.norecordsfound }}
                </td>
              </tr>
            </ng-template>
            <ng-template pTemplate="paginatorleft" let-state>
              <pagination-counts
                *ngIf="DashboardData.labeling.availableTasks?.length"
                [inputData]="DashboardData.labeling?.availableTasks"
                [inputEvent]="dashboardGridEvents.labeling?.availTasksEvent"
              ></pagination-counts>
            </ng-template>
          </p-table>
          <!--End Available Tasks -->

          <div>&nbsp;</div>

          <!-- Upcoming Tasks -->
          <app-section-header [headerLabel]="prodDashboardResource.lblUpcomingTask"></app-section-header>
          <p-table #dtLabelingUpcomingTask
            [value]="DashboardData.labeling?.upcomingTasks"
            styleClass="clsTableWithoutHeaderColorCss clsRemoveTableInnerBorders"
            [rowsPerPageOptions]="DashboardData.labeling?.upcomingTasksPagination" 
            (onPage)="onUpcomingPageChange($event)"
            [responsive]="true"
            rowHover="true"
            [paginator]="true" 
            [rows]="10"
            >
            <ng-template pTemplate="header">
              <tr>
                <th [pSortableColumn]="'StrainName'"> 
                  {{prodDashboardResource.lblStrain}}
                  <p-sortIcon [field]="'StrainName'"></p-sortIcon>
                </th>
                <th>{{prodDashboardResource.lblPkgSize}}</th>
                <th [pSortableColumn]="'OrderRefId'"> 
                  {{prodDashboardResource.lblOrder}}
                  <p-sortIcon [field]="'OrderRefId'"></p-sortIcon>
                </th>
                <th [pSortableColumn]="'TubeRequiredForLabelingQty'"> 
                  {{prodDashboardResource.lblUpcomingUnits}}
                  <p-sortIcon [field]="'TubeRequiredForLabelingQty'"></p-sortIcon>
                </th>
                <th class="clsRemoveFrmPrint clsAssignTaskColumn">&nbsp;</th>
              </tr>
              <tr class="clsRemoveFrmPrint">
                <th>
                  <input pInputText autocomplete="off" type="search" 
                    (input)="dtLabelingUpcomingTask.filter($event.target.value, 'StrainName', 'contains')"
                    class="clsControl">
                </th>
                <th>
                    &nbsp;
                </th>
                <th>
                  <input pInputText autocomplete="off" type="search" 
                  (input)="dtLabelingUpcomingTask.filter($event.target.value, 'OrderRefId', 'contains')"
                  class="clsControl">
                </th>
                <th>
                    &nbsp;
                </th>
                <th class="clsRemoveFrmPrint clsAssignTaskColumn">&nbsp;</th>
              </tr>
            </ng-template>
            <ng-template pTemplate="body" let-rowData let-i="rowIndex" >
              <tr>
                <!--Strain -->
                <td>
                  <span class="ui-column-title">{{prodDashboardResource.lblStrain}}</span>
                  {{rowData?.StrainName}}
                </td>
                <!--End Strain -->
                <!--Order Package Size -->
                <td>
                  <span class="ui-column-title">{{prodDashboardResource.lblPkgSize}}</span>
                  {{rowData?.PkgTypeName}}&nbsp;-&nbsp;{{rowData?.ItemQty}}&nbsp;x&nbsp;{{rowData?.UnitValue}} {{prodDashboardResource.unitGrams}}
                </td>
                <!--Order Package Size -->
                <!--OrderNo -->
                <td>
                    <span class="ui-column-title">{{prodDashboardResource.lblOrder}}</span>
                    {{rowData?.OrderRefId}}
                </td>
                <!--OrderNo -->
                <!--ReadyForLabeling -->
                <td>
                  <span class="ui-column-title">{{prodDashboardResource.lblReadyForLabeling}}</span>
                  {{rowData?.TubeRequiredForLabelingQty}}&nbsp;{{rowData?.PkgTypeName}}{{rowData?.TubeRequiredForLabelingQty > 1 && rowData?.PkgTypeName.toUpperCase() !== 'JOINTS' ? 's' : ''}}
                </td>
                <!--ReadyForLabeling -->
                <!--Claim Button -->
                <td class="clsRemoveFrmPrint clsAssignTaskColumn" style="text-align:center;">
                    <p-button type="Button" styleClass="ui-button-rounded disableGrayButton"
                      label="{{prodDashboardResource.btnClaim}}">
                    </p-button>
                </td>
                <!--Claim Button -->                
              </tr>
            </ng-template>
            <ng-template pTemplate="emptymessage">
              <tr>
                <td [attr.colspan]="5">
                  {{ globalResource.norecordsfound }}
                </td>
              </tr>
            </ng-template>
            <ng-template pTemplate="paginatorleft" let-state>
              <pagination-counts
                *ngIf="DashboardData.labeling?.upcomingTasks?.length"
                [inputData]="DashboardData.labeling?.upcomingTasks"
                [inputEvent]="dashboardGridEvents.labeling?.upcomingTasksEvent"
              ></pagination-counts>
            </ng-template>
          </p-table>
          <!--End Upcoming Tasks -->

        </div>
      </div>
    </div>
</div>
<!-- End Labeling Task Grid -->


